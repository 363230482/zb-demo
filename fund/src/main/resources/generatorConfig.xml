<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    
    <classPathEntry location="F:\apache-maven-3.2.5\repository\mysql\mysql-connector-java\5.1.12\mysql-connector-java-5.1.12.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" 
			connectionURL="jdbc:mysql://127.0.0.1:3306/db_funds" 
			userId="root" 
			password="root" />
			
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        
        <javaModelGenerator targetPackage="com.zb.fund.domain" 
                            targetProject="src\\main\\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        
        <sqlMapGenerator targetPackage="mappers.fund" 
                         targetProject="src\\main\\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
       
        <javaClientGenerator type="XMLMAPPER" 
                             targetPackage="com.zb.fund.mapper" 
                             targetProject="src\\main\\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        
        <table tableName="t_fund" domainObjectName="Fund" 
			enableCountByExample="false" 
			enableUpdateByExample="false" 
			enableDeleteByExample="false" 
			enableSelectByExample="false" 
			selectByExampleQueryId="false">
		</table>
        
        <table tableName="t_fund_type" domainObjectName="FundType" 
			enableCountByExample="false" 
			enableUpdateByExample="false" 
			enableDeleteByExample="false" 
			enableSelectByExample="false" 
			selectByExampleQueryId="false">
		</table>
        
        <table tableName="t_fund_trend" domainObjectName="FundTrend" 
			enableCountByExample="false" 
			enableUpdateByExample="false" 
			enableDeleteByExample="false" 
			enableSelectByExample="false" 
			selectByExampleQueryId="false">
		</table>
        
    </context>
</generatorConfiguration>